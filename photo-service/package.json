{
  "name": "photo-service",
  "version": "1.0.0",
  "description": "Photo service",
  "repository": {},
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf node_modules/ && rm -rf test-coverage/ && rm -rf test-report/ && rm -rf spec-coverage/ && rm -rf spec-report/ && rm -rf dist/ && rm -f src/routes/routes.ts",
    "lint": "npx tslint -c tslint.json 'src/**/*.ts'",
    "start": "node -r dotenv/config dist/index.js",
    "build:ts": "npx tsc",
    "watch:ts": "npx tsc -w",
    "build": "npm run build:ts",
    "test": "jest",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner"
  },
  "keywords": [
    "photo",
    "upload"
  ],
  "author": "Yura Zhunkivskyi <zhunik@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "amqplib": "^0.5.5",
    "dotenv": "^8.2.0",
    "mkdirp": "^0.5.1",
    "mongoose": "^5.7.14",
    "restify": "^8.5.0",
    "restify-errors": "^8.0.1",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-restify": "^3.0.8",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^24.0.23",
    "@types/mkdirp": "^0.5.2",
    "@types/mongoose": "^5.5.32",
    "@types/restify": "^8.4.2",
    "@types/restify-errors": "^4.3.3",
    "@types/swagger-jsdoc": "3.0.2",
    "@types/winston": "^2.4.4",
    "jest": "^24.9.0",
    "jest-html-reporter": "^2.7.0",
    "jest-junit": "^10.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "sonar-scanner": "^3.1.0",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  },
  "jestSonar": {
    "reportPath": "test-report",
    "reportFile": "test-reporter.xml",
    "indent": 4
  }
